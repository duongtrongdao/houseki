<div class="center">
    <div class="col-md-12">
        <% @fooddrinks.each do |fooddrink| %>
            <div class="col-md-4">
                <%= link_to fooddrink_path(fooddrink.id) do %>
                    <%= image_tag(fooddrink.file, class: "img-thumbnail cover") %>
                <% end %>
            </div>
        <% end %>
    </div>
    <div class="col-md-6 col-md-offset-3">
        <%= will_paginate @fooddrinks %>
    </div>
</div>