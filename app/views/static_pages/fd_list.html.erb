<link rel="stylesheet" href="//demo.neontheme.com/assets/css/bootstrap.css" >
<link rel="stylesheet" href="//demo.neontheme.com/assets/css/neon-core.css" >
<link rel="stylesheet" href="//demo.neontheme.com/assets/css/font-icons/entypo/css/entypo.css">


<style>
body {
   background-image: none !important;
}
.container {
  background: none !important;
}

.container {
  background: transparent !important;
}

</style>

<script>
    $(function(){
        $('.page-body').parent().addClass('head-img');
    })
    </script>
<p id="notice"><%= notice %></p>

<div class="page-body" style="position:relative;display:flex;">
  
    <div class="col-md-3 left-side" style=""><!--position:fixed; margin-top: 30px;-->
      <div class="page-container sidebar-collapsed" style=" width: auto;">
      <%= render 'layouts/side' %>
    </div>
  </div>
  
<!--<div class="page-container sidebar-collapsed">-->
<!--  <div class="col-md-3 left-side">-->
<!--    </%= render 'layouts/side' %>-->
<!--  </div>-->
  
  <div class="col-md-9 right-side" style="background-color:#333;">
    <h1 style="color:#aaabae;margin-left:20px;"><%= @fd_type.capitalize %> List</h1>
    <div class="col-md-12">
        <% @fdall.each do |fd| %>
            <div class="col-md-4">
                <%= link_to fooddrink_path(fd.id), :rel => "lightbox", class: "has-tooltip", :data => {:toggle=>"tooltip", :placement=>"auto"}, title: display_info(fd) do %>
                    <%= image_tag(fd.file, class: "img-thumbnail avatar") %>
                <% end %>
            </div>
            <!--<h4 class="div-co-name"></%= fd.name %></h4>-->
        <% end %>
        
    </div>
    <div class="col-md-6 col-md-offset-3">
        <%= will_paginate @fdall %>
    </div>
  </div>
</div>