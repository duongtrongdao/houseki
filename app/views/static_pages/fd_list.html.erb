<h1><%= @fd_type.capitalize %> List</h1>

<div class="row">
    <div class="col-md-12">
        <% @fdall.each do |fd| %>
            <div class="col-md-4">
                <%= link_to fooddrink_path(fd.id) do %>
                    <%= image_tag(fd.file, class: "img-thumbnail avatar") %>
                <% end %>
            </div>
        <% end %>
    </div>
    <div class="col-md-6 col-md-offset-3">
        <%= will_paginate @fdall %>
    </div>
</div>