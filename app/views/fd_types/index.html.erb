<p id="notice"><%= notice %></p>

<h1>Listing Fd Types</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Foodtype</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @fd_types.each do |fd_type| %>
      <tr>
        <td><%= fd_type.name %></td>
        <td><%= fd_type.foodtype %></td>
        <td><%= link_to 'Show', fd_type %></td>
        <td><%= link_to 'Edit', edit_fd_type_path(fd_type) %></td>
        <td><%= link_to 'Destroy', fd_type, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Fd type', new_fd_type_path %>
